{
  "name": "create-nhollas-app",
  "version": "auto",
  "keywords": [
    "react",
    "next",
    "next.js"
  ],
  "description": "Create Next.js-powered React apps with one command",
  "author": "Nhollas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nhollas/create-nhollas-app.git"
  },
  "bin": {
    "create-nhollas-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts --no-splitting",
    "dev": "tsup index.ts --format cjs,esm --watch --no-splitting",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/node": "^20.14.2",
    "@types/validate-npm-package-name": "4.0.2",
    "commander": "12.1.0",
    "cross-spawn": "7.0.3",
    "fast-glob": "3.3.1",
    "picocolors": "1.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5",
    "validate-npm-package-name": "5.0.1"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
